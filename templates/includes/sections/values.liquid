<div id="values">
	<div class="content">
		<h1>{{global.index.sections[0].title}}</h1>
		<ul class="values">
		{% for item in global.index.sections[0].values %}
			<li>
				<span class="icon ">{{item.icon}}</span>
				<h3>{{item.title}}</h3>
				<p>{{item.description}}</p>
			</li>
		{% endfor %}
		</ul>
	</div>
</div>