<div id="team">
	<div class="content">
		<h1>{{global.index.sections[1].title}}</h1>
		<ul class="team">
		{% for item in global.index.sections[1].team %}
			<li>
				<span class="avatar "><img src="{{item.imageUrl}}" alt="{{item.name}}"></span>
				<h3>{{item.name}}</h3>
				<p>{{item.role}}</p>
			</li>
		{% endfor %}
		</ul>
	</div>
</div>